            <entry>pre</entry>
            <entry>nowrap</entry>
          </entries>
        </element>
        <element name="word-spacing">
          <entries>
            <entry>length...</entry>
            <entry>normal</entry>
          </entries>
        </element>
      </group>
    </collection>

