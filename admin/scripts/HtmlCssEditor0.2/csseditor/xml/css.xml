<?xml version="1.0" encoding="UTF-8"?>
<css xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="css.xsd">
  <collections>

    <collection name="Background">
      <group name="Background">
        <element name="background-attachment">
          <entries>
            <entry>scroll</entry>
            <entry>fixed</entry>
          </entries>
        </element>
        <element name="background-color">
          <entries>
            <entry>color...</entry>
            <list>colors</list>
          </entries>
        </element>
        <element name="background-image">
          <entries>
            <entry>file...</entry>
          </entries>
        </element>
        <element name="background-position">
          <entries>
            <entry>position...</entry>
            <entry>top left</entry>
            <entry>top center</entry>
            <entry>top right</entry>
            <entry>center left</entry>
            <entry>center center</entry>
            <entry>center right</entry>
            <entry>bottom left</entry>
            <entry>bottom center</entry>
            <entry>bottom right</entry>
          </entries>
        </element>
        <element name="background-repeat">
          <entries>
            <entry>repeat</entry>
            <entry>repeat-x</entry>
            <entry>repeat-y</entry>
            <entry>no-repeat</entry>
          </entries>
        </element>
      </group>
    </collection>

